    "express-slash": "^2.0.1",
    "express-state": "^1.2.0",
    "glob": "^5.0.3",
    "handlebars": "^4.0.11",
    "handlebars-intl": "^1.1.1",
    "intl": "^0.1.4",
    "intl-locales-supported": "^1.0.0",
    "jquery": "^3.3.1",
    "js-yaml": "^3.2.1",
    "jumanji": "^0.1.1",
    "morgan": "^1.2.3",
    "nodeify": "^1.0.0",
